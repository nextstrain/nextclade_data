{
  "$schema": "https://raw.githubusercontent.com/nextstrain/nextclade/refs/heads/release/packages/nextclade-schemas/input-pathogen-json.schema.json",
  "alignmentParams": {
    "minSeedCover": 0.01,
    "kmerLength": 7,
    "allowedMismatches": 10,
    "retryReverseComplement": true,
    "penaltyGapOpen": 24,
    "penaltyGapOpenInFrame": 26,
    "penaltyGapOpenOutOfFrame": 28
  },
  "files": {
    "reference": "reference_M.fasta",
    "pathogenJson": "pathogen.json",
    "genomeAnnotation": "genome_annotation.gff3",
    "treeJson": "CCHF_M.json",
    "examples": "sample_M.fasta",
    "readme": "README.md",
    "changelog": "CHANGELOG.md"
  },
  "schemaVersion": "3.0.0",
  "attributes": {
    "name": "CCHF (segment M)",
    "reference name": "Crimean-Congo hemorrhagic fever virus segment M",
    "reference accession": "NC_005300.2"
  },
  "qc": {
    "privateMutations": {
      "enabled": true,
      "typical": 50,
      "cutoff": 200,
      "weightLabeledSubstitutions": 2,
      "weightReversionSubstitutions": 2,
      "weightUnlabeledSubstitutions": 1
    },
    "missingData": {
      "enabled": false,
      "missingDataThreshold": 3000,
      "scoreBias": 500
    },
    "snpClusters": {
      "enabled": false,
      "windowSize": 100,
      "clusterCutOff": 12,
      "scoreWeight": 50
    },
    "mixedSites": {
      "enabled": true,
      "mixedSitesThreshold": 8
    },
    "frameShifts": {
      "enabled": true,
      "ignoredFrameShifts": []
    },
    "stopCodons": {
      "enabled": true,
      "ignoredStopCodons": []
    }
  },
  "mutLabels": {
    "nucMutLabelMap": {}
  },
  "version": {
    "tag": "unreleased"
  }
}
