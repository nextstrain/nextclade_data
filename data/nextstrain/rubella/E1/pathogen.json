      "cutoff": 12,
      "typical": 4,
      "weightLabeledSubstitutions": 1,
      "weightReversionSubstitutions": 1,
      "weightUnlabeledSubstitutions": 1
    },
    "snpClusters": {
      "enabled": true,
      "clusterCutOff": 3,
      "scoreWeight": 50,
      "windowSize": 50
    }
  }
